{
  "name": "yulianabuitrago.com",
  "description": "Yuliana's Portfolio page <https://www.linkedin.com/in/yulianabglez>",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Marcos Beneyto <https://www.linkedin.com/in/mbeneytofdez>",
  "homepage": "https://github.com/mbeneyto/yulianabuitrago.com#readme",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:esm && npm run copy",
    "build:js": "babel src/ -d dist",
    "build:js:watch": "babel src/ -d dist --watch",
    "build:esm": "snowpack --include \"dist/**/*.js\" --dest dist/web_modules",
    "copy": "cpx index.html ./dist",
    "dev": "npm run build && npm run build:js:watch & serve -s dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbeneyto/yulianabuitrago.com.git"
  },
  "dependencies": {
    "construct-style-sheets-polyfill": "^2.3.5",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "cpx": "^1.5.0",
    "snowpack": "^1.5.0"
  },
  "keywords": [],
  "snowpack": {
    "webDependencies": [
      "lit-html",
      "lit-element",
      "lit-html/directives/until.js"
    ],
    "dedupe": [
      "lit-element",
      "lit-html"
    ]
  }
}
